<div
  id="case-conference-page"
  class="modal fade"
  data-bs-backdrop="static"
  data-bs-keyboard="false"
  tabindex="-1"
  role="dialog"
  aria-labelledby="case-conference-page-modalTitle"
  aria-hidden="true"
>
  <div class="modal-dialog modal-xl">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="case-conference-page-modalTitle"></h5>
        <button
          type="button"
          class="btn-close"
          data-bs-dismiss="modal"
          aria-label="Close"
        ></button>
      </div>
      <div class="modal-body">
        <div class="card">
          <div class="card-header">
            <div class="row">
              <div class="col-md-12 text-end">
                <a href="#" class="btn btn-light" id="btn-new-case-conference">
                  <i class="bx bx-plus me-1"></i> Add New
                </a>
              </div>
            </div>
          </div>
          <div class="card-body">
            <div class="row case-conference-cardBody"></div>
          </div>
        </div>
      </div>
      <div class="modal-footer">
        <button
          type="button"
          class="btn btn-secondary waves-effect"
          data-bs-dismiss="modal"
        >
          Close
        </button>
      </div>
    </div>
  </div>
</div>
<%- include ('./case-conference-modal-form') %>
